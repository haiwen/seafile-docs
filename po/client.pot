msgstr ""

#: client/seafile_cli.md:15
# header
msgid "start-ccnet\n"
"-----------"
msgstr ""

#: client/seafile_cli.md:17
msgid "Start ccnet daemon"
msgstr ""

#: client/seafile_cli.md:19
msgid "usage: seaf-cli -c <config-dir> -o start-ccnet"
msgstr ""

#: client/seafile_cli.md:21
# header
