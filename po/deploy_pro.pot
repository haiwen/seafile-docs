"[commit_object_backend]\n"
"name = swift\n"
"tenant = yourTenant\n"
"user_name = user\n"
"password = secret\n"
"container = seafile-commits\n"
"auth_host = 192.168.56.31:5000\n"
"auth_ver = v2.0\n"
"memcached_options = --SERVER=192.168.1.134:11211 --POOL-MIN=10 --POOL-MAX=100\n"
"\n"
"[fs_object_backend]\n"
"name = swift\n"
"tenant = yourTenant\n"
"user_name = user\n"
"password = secret\n"
