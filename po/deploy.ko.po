"\n"
"        access_log      /var/log/nginx/seahub.access.log;\n"
"    \terror_log       /var/log/nginx/seahub.error.log;\n"
"    \tfastcgi_read_timeout 36000;\n"
"    }\n"
"\n"
"    location /seafhttp {\n"
