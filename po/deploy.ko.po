# inline html
#: deploy/upgrade_with_mysql_or_postgresql.md:6
msgid ""
"<pre>\n"
"haiwen\n"
"   -- seafile-server-1.5.0\n"
"   -- seafile-server-1.6.0\n"
"   -- ccnet\n"
"   -- seafile-data\n"
"</pre>"
msgstr ""
"<pre>\n"
"haiwen\n"
"   -- seafile-server-1.5.0\n"
"   -- seafile-server-1.6.0\n"
"   -- ccnet\n"
"   -- seafile-data\n"
